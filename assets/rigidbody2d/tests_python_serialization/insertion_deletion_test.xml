<rigidbody2d_scene>

  <camera center="0 0" scale="11.2642" fps="50" render_at_fps="0" locked="0"/>

  <scripting callback="deletion_test"/>

  <integrator type="verlet" dt="0.02"/>

  <sobogus_friction_solver mu="0.3" CoR="0.7" max_iters="50000" eval_every="25" tol="1.0e-6" staggering="geometric" cache_impulses="none"/>

  <near_earth_gravity f="0.0 -10.0"/>

  <static_plane x="0 -5" n="0 1"/>
  <static_plane x="-10 0" n="1 0"/>
  <static_plane x="10 0" n="-1 0"/>

  <geometry type="circle" r="0.5"/>

  <rigid_body x="0 -2" theta="0.5" v="0.5 1.0" omega="2.0" rho="1" geo_idx="0" fixed="0"/>

</rigidbody2d_scene>
