<rigidbody2d_scene>
  <camera center="0 0" scale="8.3225" fps="60" render_at_fps="0" locked="0"/>

  <!-- this seems to conserve momentum better -->
  <integrator type="verlet" dt="0.1"/>

  <impact_operator type="gr" CoR="1" v_tol="0.5e-9" cache_impulses="0">
    <!-- <solver name="policy_iteration" max_iters="0.5" tol="0.5e-8"/> -->
    <solver name="lcp_solver_isaiah_debug" linear_solvers="ma27 ma86" max_iters="100" tol="0.5e-12"/>
  </impact_operator>

  <!-- no gravity -->
  <near_earth_gravity f="0.0 0.0"/>

  <!-- r = sqrt(2) -->
  <geometry type="circle" r="1"/>


